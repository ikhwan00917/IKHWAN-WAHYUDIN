    private void ConvertButtonActionPerformed(java.awt.event.ActionEvent evt) {                                              
double Celcius = Double.parseDouble(CelciusTextField.getText());
double Fahrenheit =Celcius * 1.8 + 32;
     FahrenheitLabel.setText(Fahrenheit + " Fahrenheit");
    }                                             

    private void ConvertButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                               
double Celcius = Double.parseDouble(CelciusTextField.getText());
double Kelvin = Celcius + 273.15;
    KelvinLabel.setText(Kelvin + " Kelvin");
    }                                              

    private void ConvertButton2ActionPerformed(java.awt.event.ActionEvent evt) {                                               
double Celcius = Double.parseDouble(CelciusTextField.getText());
double Reamur =Celcius * 8;
     ReamurLabel.setText(Reamur + " Reamur");
    }                                              

    private void ConvertFhcActionPerformed(java.awt.event.ActionEvent evt) {                                           
double Fahrenheit = Double.parseDouble(FahrenheitTextField.getText());
double Celcius =(Fahrenheit - 32)/1.8;
     CelciusLabel1.setText(Celcius + " Celcius");
    }                                          

    private void ConvertFhkButtonActionPerformed(java.awt.event.ActionEvent evt) {                                                 
double Fahrenheit = Double.parseDouble(FahrenheitTextField.getText());
double Kelvin =(Fahrenheit + 459.67)/1.8;
     KelvinLabel1.setText(Kelvin + " Kelvin");
    }                                                

    private void ConvertFhrActionPerformed(java.awt.event.ActionEvent evt) {                                           
double Fahrenheit = Double.parseDouble(FahrenheitTextField.getText());
double Reamur =(Fahrenheit - 32)/2.25;
     ReamurLabel1.setText(Reamur + " Reamur");
    }                                          

    private void ConverrtKcButtonActionPerformed(java.awt.event.ActionEvent evt) {                                                 
double Kelvin = Double.parseDouble(KelvinTextField.getText());
double Celcius =Kelvin - 273.15;
     CelciusLabel2.setText(Celcius + " Celcius");
    }                                                

    private void ConvertKfButtonActionPerformed(java.awt.event.ActionEvent evt) {                                                
double Kelvin = Double.parseDouble(KelvinTextField.getText());
double Fahrenheit =Kelvin * 1.8 - 459.67;
     FahrenheitLabel2.setText(Fahrenheit + " Fahrenheit");
    }                                               

    private void ConvertKrButtonActionPerformed(java.awt.event.ActionEvent evt) {                                                
double Kelvin = Double.parseDouble(KelvinTextField.getText());
double Reamur =(Kelvin - 273.15)*0.8;
     ReamurLabel2.setText(Reamur + " Reamur");
    }                                               

    private void ConvertRcButtonActionPerformed(java.awt.event.ActionEvent evt) {                                                
double Reamur = Double.parseDouble(ReamurTextField.getText());
double Celcius =Reamur/8;
     CelciusLabel3.setText(Celcius + " Celcius");
    }                                               

    private void ConvertRfButtonActionPerformed(java.awt.event.ActionEvent evt) {                                                
double Reamur = Double.parseDouble(ReamurTextField.getText());
double Fahrenheit =(Reamur - 7.5)*24/7+32;
     FahrenheitLabel3.setText(Fahrenheit + " Fahrenheit");
    }                                               

    private void ConvertRkButtonActionPerformed(java.awt.event.ActionEvent evt) {                                                
double Reamur = Double.parseDouble(ReamurTextField.getText());
double Kelvin =Reamur/8+273.15;
     KelvinLabel3.setText(Kelvin + " Kelvin");
    }                                               

